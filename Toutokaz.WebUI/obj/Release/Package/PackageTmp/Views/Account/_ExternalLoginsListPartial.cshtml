@model ICollection<AuthenticationClientData>

@if (Model.Count == 0)
{
    
}
else
{
    using (Html.BeginForm("ExternalLogin", "Account", new { ReturnUrl = ViewBag.ReturnUrl }))
    {
    @Html.AntiForgeryToken()
    <div id="socialLoginList">
        <p>
        @foreach (AuthenticationClientData p in Model)
        {
              object src = @p.ExtraData["Icon"];
              <input type="image" src="@Url.Content(src.ToString())" alt="Icon for @p.DisplayName" name="provider" value="@p.AuthenticationClient.ProviderName" title="Se connecter avec mon compte @p.DisplayName"/>
            //  <img src="@Url.Content(src.ToString())" alt="Icon for @p.DisplayName" />
           //   <button class="btn btn-primary btn-lg btn-block"  type="submit" name="provider" value="@p.AuthenticationClient.ProviderName" title="Se connecter avec mon compte @p.DisplayName">Se connecter avec @p.DisplayName</button>
        }
        </p>
    </div>
    }
}
